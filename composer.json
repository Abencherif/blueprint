{
    "name": "laravie/blueprint",
    "description": "API Blueprint documentation generator.",
    "keywords": ["blueprint", "api", "laravel", "dingo", "docs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Lewis",
            "email": "jason.lewis1991@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.7 || ^6.0",
        "illuminate/filesystem": "^5.7 || ^6.0",
        "doctrine/annotations": "^1.2",
        "phpdocumentor/reflection-docblock": "^3.1 || ^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dingo\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dingo\\Blueprint\\Tests\\": "tests"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
